<script>
  jQuery(document).ready(function (){
    $('[data-scrolld]').scrolld({
      speed: 750,
      offset: -66,
      easing: 'swing'
    });
    jQuery('#topbanner').revolution({
//      delay: 4000,
      startwidth: 1170,
      startheight: 700,
      hideThumbs: 10
    });
    jQuery('#fansbanner').revolution({
//      delay: 5000,
      startwidth: 1170,
      startheight: 700,
      hideThumbs: 10
    });
    $('#new-timeline').slider({
      range: true,
      min: 0,
      max: 50,
      step: 10,
      animate: "fast",
      values: [10, 40 ],
      slide: function (event, ui){
        var handles = $(this).children('.ui-slider-handle'),
            handleLeft = handles.eq(0),
            handleRight = handles.eq(1);
        if(ui.values[ 0 ] == ui.values[1]) {
          if(ui.values[ 0 ] == 0) {
            handleLeft.css('marginLeft', '5px');
            handleRight.css('marginLeft', '5px');
          }
          else if(ui.values[ 0 ] == 5) {
            handleLeft.css('marginLeft', '-26px');
            handleRight.css('marginLeft', '-26px');
          }
          else {
            var ml = $(this).children('.ui-slider-handle').not(ui.handle).css('marginLeft')
            handleLeft.css('marginLeft', ml);
            handleRight.css('marginLeft', ml);
          }
        }
        else {
          handleLeft.css('marginLeft', '5px');
          handleRight.css('marginLeft', '-26px');
        }
        var dates = $('.timeline-date').children('span');
        dates.not(dates.slice(ui.values[ 0 ] / 10, ui.values[ 1 ] / 10 + 1).addClass('active')).removeClass('active');
        $("#amount").val("$" + ui.values[ 0 ] + " - $" + ui.values[ 1 ]);
      }
    });
  });
</script>